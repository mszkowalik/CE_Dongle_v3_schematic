(version 1)

(rule "Courtyard-to-Edge 1mm (F)"
  (layer "F.Courtyard")
  (constraint physical_clearance (min 1mm))
  (condition "B.Layer == 'Edge.Cuts' && !A.memberOfFootprint('J*')"))

(rule "Courtyard-to-Edge 1mm (B)"
  (layer "B.Courtyard")
  (constraint physical_clearance (min 1mm))
  (condition "B.Layer == 'Edge.Cuts' && !A.memberOfFootprint('J*')"))